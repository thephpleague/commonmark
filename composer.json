{
    "name": "league/commonmark-ext-task-list",
    "type": "commonmark-extension",
    "description": "Extension for league/commonmark which supports GFM-style task lists",
    "keywords": ["markdown", "commonmark", "extension", "github", "gfm", "task", "lists"],
    "homepage": "https://github.com/thephpleague/commonmark-ext-task-list",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "colinodell@gmail.com",
            "homepage": "https://www.colinodell.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php" : "^7.1",
        "league/commonmark": "^0.19|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "League\\CommonMark\\Ext\\TaskList\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\CommonMark\\Ext\\TaskList\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
